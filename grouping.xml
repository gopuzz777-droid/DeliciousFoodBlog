<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Grouped Test Suite">

  <!-- Listener -->
  <listeners>
    <listener class-name="utils.ExtentReportManager"/>
  </listeners>

  <!-- Test -->
  <test name="ChromeTest">
    <parameter name="browser" value="chrome"/>

    <groups>
      <run>
        <include name="sanity"/>
                <include name="regression"/>

      </run>
    </groups>

    <classes>
      <class name="testCases.TC_01AccountRegistration"/>
      <class name="testCases.TC_02TestLogin"/>
            <class name="testCases.TC_03SearchFuntion"/>
                <class name="testCases.TC_04FilterFunction"/>
 					<class name="testCases.TC_05SaveRecipe"/>


      
    </classes>
  </test>

</suite>
